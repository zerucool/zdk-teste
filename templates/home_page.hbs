<section class="hero hero-large">
  <img src="{{settings.homepage_background_image}}" class="blur banner">
  <div class="hero-inner anonymous">
    <h1 class="hero-inner-home-heading">DataSource Knowledge Base</h1>
    <div class="hero-inner-description">
      <p>Please log into the DataSource Extranet to view this content.</p>
      <p><a href="http://extranet.cnetcontent.com" target="_blank" class="button button-large"><span>Login</span></a><span>&nbsp;&nbsp;or&nbsp;&nbsp;</span><a href="http://cnetcontent.com/register" target="_blank" class="button button-large button-secondary"><span>Register</span></a></p>
    </div>
  </div>
  <div class="hero-inner customer">
    <h1 class="hero-inner-home-heading">ðŸ‘‹ Hi. How can we help?</h1>
    <div class="hero-inner-search">
      {{search placeholder="Find answers" submit=false scoped=settings.scoped_kb_search instant=settings.instant_search class="search search-full"}}
    </div>
    <p class="hero-inner-description">Started with the <a href="/hc/en-us/articles/360045726612" target="_self"><u>Extranet Introduction</u></a> and <a href="/hc/en-us/articles/360045728832-Your-Account" target="_self"><u>Your Account</u></a> videos.</p>
  </div>
</section>

<div class="container">


  <section class="anonymous message">
    <p>If you have a DataSource account but have never used the Extranet, please register and a representive will help you set up your account. Learn more about <a href="http://cnetcontent.com/datasource" target="_blank"><u>DataSource</u></a> or <a href="https://extranetsandbox.zendesk.com/hc/en-us/requests/new" target="_blank">contact a representive</a> for more details.</p>
  </section>

  <section class="customer section knowledge-base">
    <h1 class="home-categories-heading" id="browse-knowledge-base">Browse Sections</h1>
    <section class="categories blocks">
      <ul class="blocks-list">
        {{#each categories}}
        {{#if ../has_multiple_categories}}
        <li class="blocks-item">
          <a href='{{url}}' class="blocks-item-link">
            {{#is name 'Quickstart Guides'}}<i class="fas fa-stopwatch fa-2x"></i>{{/is}}
            {{#is name 'Documentation'}}<i class="fas fa-book-reader fa-2x"></i>{{/is}}
            {{#is name 'Release Notes'}}<i class="fas fa-list-ul fa-2x"></i>{{/is}}
            {{#is name 'Technical Bulletin'}}<i class="fas fa-align-left fa-2x"></i>{{/is}}
            {{#is name 'FAQs'}}<i class="fas fa-comments fa-2x"></i>{{/is}}
            {{#is name 'Video Training'}}<i class="fas fa-video fa-2x"></i>{{/is}}
            <h2 class="blocks-item-title">{{name}}</h2>
            <p class="blocks-item-description">{{excerpt description}}</p>
          </a>
        </li>
        {{else}}
        {{#each sections}}
        <li class="blocks-item">
          <a class="blocks-item-link" href='{{url}}'>
            <h2 class="blocks-item-title">{{name}}</h2>
            <p class="blocks-item-description">{{excerpt description}}</p>
          </a>
        </li>
        {{/each}}
        {{/if}}
        {{/each}}
      </ul>
      {{pagination}}
    </section>

  {{!-- #if promoted_articles}}
  <section class="articles">
    <h3>{{t 'promoted_articles'}}</h3>
    <ul class="article-list promoted-articles">
      {{#each promoted_articles}}
      <li class="promoted-articles-item">
        <a href="{{url}}">
          {{title}}
          {{#if internal}}
          <span class="icon-lock" title="{{t 'internal'}}"></span>
          {{/if}}
        </a>
      </li>
      {{/each}}
    </ul>
  </section>
  {{/if --}}

  {{!-- #if help_center.community_enabled}}
  <section class="section community">
    <h2>{{t 'community'}}</h2>
    {{#link 'community' class='community-link'}}
    {{t 'join_conversation'}}
    {{/link}}
    <div class="community-image"></div>
  </section>
  {{/if --}}

  {{!-- #if settings.show_recent_activity}}
  {{<section class="section activity">
    {{recent_activity}}
  </section>}}
  {{/if --}}

</div>


