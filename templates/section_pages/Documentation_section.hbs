
<section class="page-header">
  <div class="page-header-inner">
    <nav class="sub-nav">
      {{breadcrumbs}}
    </nav>
    <div class="page-header-info">
      <h1>{{section.name}}</h1>
      {{#if settings.show_follow_section}}
        <div class="page-header-subscribe">{{subscribe}}</div>
      {{/if}}
      {{#if section.description}}
        <p class="page-header-description">{{section.description}}</p>
      {{/if}}
    </div>
  </div>
</section>


<section id="TemplateSection" class="ccs-doc-page">
  <article class="ccs-doc-content">

    <div class="row">
    {{#if section.sections}}
      {{#each section.sections}}
      {{#isnt id 360009212532}} {{!-- Hidding sub section (container) for not the main articles --}}
        <div class="sm-12 md-6 lg-4 col" style="padding: 0 0 3.2rem">
          <h3>{{name}}</h3>
          {{#if articles}}
            <ul>
               {{#each articles}}
                <li class="{{#if promoted}}promoted{{/if}}">
                  {{!--When user click in the document section on article file will start downlaoding. previues <a href="{{url}}" class="{{#if internal}}internal{{/if}}" </a> --}}
                  <a href= "{{#is id 360042011331}} https://download.cnetcontentsolutions.com/dropbox/User_Manuals/Category_List.xlsx
                            {{else}}{{#is id 360041579472}} https://download.cnetcontentsolutions.com/dropbox/User_Manuals/Manufacturer_List.xlsx
                            {{else}}{{#is id 360044982631}} https://download.cnetcontentsolutions.com/dropbox/User_Manuals/ACC_Previous_Current_Draft.xlsx
                            {{else}} {{url}} {{/is}}{{/is}}{{/is}}" class="{{#if internal}}internal{{/if}}">{{title}}</a>
                </li>
               {{/each}}
            </ul>
          {{#if more_articles}}
            <br><a href="{{url}}" class="ccs-btn-subtle">{{t 'show_all_articles' count=article_count}}</a>
           {{!--  {{else}}
           <br><a href="{{url}}" class="ccs-btn-subtle">View Section</a> removing link on the sub subcategory --}}
          {{/if}}
          {{/if}}
        </div>
      {{/isnt}} {{!-- end-Hidding sub section (container) for not the main articles --}}
      {{/each}}
    {{/if}}
  </div>



 {{!-- <!--
  {{#if section.sections}}
   <ul class="ccs-section-list">
     {{#each section.sections}}
     <li><a href="{{url}}">{{name}}</a><i class="material-icons">chevron_right</i></li>
     {{/each}}
   </ul>
   <br>
  {{/if}}
  --> --}}

    {{#if section.articles}}
    <div class="section-tree">
        <ul class="article-list">
          {{#each section.articles}}
               {{!-- {{#isnt id 360044592372}} --}}
            <li class="article-list-itemCNET Section-articaleCNET {{#if promoted}} article-promoted{{/if}}">
              {{#if promoted}}
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" focusable="false" viewBox="0 0 12 12" class="icon-star" title="{{t 'promoted'}}">
                  <path fill="currentColor" d="M2.88 11.73c-.19 0-.39-.06-.55-.18a.938.938 0 01-.37-1.01l.8-3L.35 5.57a.938.938 0 01-.3-1.03c.12-.37.45-.63.85-.65L4 3.73 5.12.83c.14-.37.49-.61.88-.61s.74.24.88.6L8 3.73l3.11.17a.946.946 0 01.55 1.68L9.24 7.53l.8 3a.95.95 0 01-1.43 1.04L6 9.88l-2.61 1.69c-.16.1-.34.16-.51.16z"/>
                </svg>
              {{/if}}   {{!--When user click in the sub section on article file will start downlaoding. previues <a href="{{url}}" {{#if promoted}} class="promoted"{{/if}}</a> --}}
               <a href= "{{#is id 360042011331}} https://download.cnetcontentsolutions.com/dropbox/User_Manuals/Category_List.xlsx
                            {{else}}{{#is id 360041579472}} https://download.cnetcontentsolutions.com/dropbox/User_Manuals/Manufacturer_List.xlsx
                            {{else}}{{#is id 360044982631}} https://download.cnetcontentsolutions.com/dropbox/User_Manuals/ACC_Previous_Current_Draft.xlsx
                            {{else}} {{url}} {{/is}}{{/is}}{{/is}}" {{#if promoted}} class="promoted"{{/if}}>{{title}}</a>
              {{#if internal}}
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" focusable="false" viewBox="0 0 16 16" class="icon-lock" title="{{t 'internal'}}">
                  <rect width="12" height="9" x="2" y="7" fill="currentColor" rx="1" ry="1"/>
                  <path fill="none" stroke="currentColor" d="M4.5 7.5V4a3.5 3.5 0 017 0v3.5"/>
                </svg>
              {{/if}}
            </li>
         {{!--         {{/isnt}} --}}
          {{/each}}
        </ul>
       </div>
      {{/if}}


  </article>

  {{pagination}}
</section>


