{
  "name": "datasource-extranet-theme",
  "version": "0.3.2",
  "description": "CCS Theme for Zendesk Guide",
  "license": "ISC",
  "homepage": "https://github.com/CNETContentSolutions/Zendesk-Theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CNETContentSolutions/Zendesk-Theme.git"
  },
  "scripts": {
    "preview": "zat theme preview",
    "prettier:write": "prettier --write \"./**/*.{js,json,scss,md}\"",
    "prettier:check": "prettier --check \"./**/*.{js,json,scss,md}\"",
    "styles:watch": "sass styles/index.scss:style.css --watch",
    "styles:build": "sass styles/index.scss:style.css --style compressed && postcss style.css --use autoprefixer --replace --no-map"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "browserslist": "^4.16.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.2",
    "sass": "^1.22.2"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2
  }
}
